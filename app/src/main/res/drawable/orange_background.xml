<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/light_orange"/>
        </shape>
    </item>
    <item
        android:width="40dp"
        android:height="40dp"
        android:top="3dp"
        android:left="20dp"
        android:gravity="left">
        <bitmap android:src="@drawable/escudo_cadereyta" />
    </item>
    <item
        android:width="40dp"
        android:height="40dp"
        android:top="3dp"
        android:right="20dp"
        android:gravity="right">
        <bitmap android:src="@drawable/logo_definitivo" />
    </item>
</layer-list>