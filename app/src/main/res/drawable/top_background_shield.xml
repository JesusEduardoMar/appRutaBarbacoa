<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:drawable="@drawable/barbacoa_im"
        android:layout_width="0dp" />
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#E6FFAC33"/>
        </shape>
    </item>
    <item
        android:width="60dp"
        android:height="60dp"
        android:top="20dp"
        android:right="20dp"
        android:gravity="right">
        <bitmap android:src="@drawable/escudo_cadereyta" />
    </item>
    <item
        android:width="60dp"
        android:height="60dp"
        android:top="20dp"
        android:left="20dp"
        android:gravity="left">
        <bitmap android:src="@drawable/logo_appp" />
    </item>
</layer-list>
