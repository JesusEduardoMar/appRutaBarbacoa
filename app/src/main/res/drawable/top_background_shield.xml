<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:drawable="@drawable/fondobarratop"
        android:layout_width="0dp" />
    <item>
        <shape android:shape="rectangle">
            <gradient
                android:startColor="#000000"
                android:endColor="@color/naranja5"
                android:angle="270"/>
        </shape>
    </item>
    <item
        android:width="60dp"
        android:height="60dp"
        android:top="20dp"
        android:right="20dp"
        android:gravity="right">
        <bitmap android:src="@drawable/logo_definitivo" />
    </item>
    <item
        android:width="60dp"
        android:height="60dp"
        android:top="20dp"
        android:left="20dp"
        android:gravity="left">
        <bitmap android:src="@drawable/escudo_cadereyta" />
    </item>
</layer-list>
